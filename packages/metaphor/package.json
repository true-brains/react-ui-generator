{
  "name": "@react-ui-generator/metaphor",
  "version": "0.0.0",
  "description": "DSL for easing baking of metadata for the react-ui-generator.",
  "main": "./out/metaphor.js",
  "types": "./out/index.d.ts",
  "repository": "https://github.com/react-ui-generator/react-ui-generator/packages/metaphor",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "doc:build": "typedoc --out ./docs/ src/index.ts"
  },
  "keywords": [
    "metadata",
    "dsl",
    "react-ui-generator"
  ],
  "author": "A.Zaviruha <a.zaviruha@gmail.com>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@react-ui-generator/core": "^0.6.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.1"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.4",
    "@types/lodash.merge": "^4.6.4",
    "jest": "^23.5.0",
    "typescript": "^3.0.3"
  }
}
